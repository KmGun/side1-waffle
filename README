    CREATE TABLE masterCal(
    id INT NOT NULL AUTO_INCREMENT,
    calId VARCHAR(100) NOT NULL,
    calName VARCHAR(100) NOT NULL,
    subscribers TEXT NULL,
    hashtags TEXT NULL,
    PRIMARY KEY(id)
    );
    CREATE TABLE users(
    id INT NOT NULL AUTO_INCREMENT,
    userId VARCHAR(100) NOT NULL,
    subscribes TEXT NULL,
    PRIMARY KEY(id)
    );
    
